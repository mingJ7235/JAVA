package p9;

public class A {
	//클래스 안에 넣을 수 있는 멤버
	int number;
	static int snumber;
	static final String NAME = "김민재";
	int [] score = new int [10];
	public A() { }
	void method () {
		
	}
	static void smethod () {
		
	}
	
	//클래스도 쓸 수 있음 (내부 클래스)
	class B {
		int bnumber;
//		static int sbnumber;
		static final String SNAME = "룰라";
		int [] bscore = new int [10];
		public B() {

		}
		void bmethod () {
			number = 50;
			snumber = 50;
			System.out.println(NAME);
			score[9] = 20;
			method ();
		}
//		static void sbmethod () {
//			
//		}
		
		//클래스 안에, 클래스 안에, 클래스 가능
		class C {
			
		}
	}
}
